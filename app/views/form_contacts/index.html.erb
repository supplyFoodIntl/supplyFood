<%= render 'management/tab' %>
<p id="notice"><%= notice %></p>

<h2>Pending messages</h2>

<table  class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Subject</th>
      <th scope="col">Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @form_contacts.each do |form_contact| %>
      <tr scope="row">
        <td><%= form_contact.name %></td>
        <td><%= form_contact.subject %></td>
        <td><%= form_contact.message %></td>
        <td><%= link_to 'Show', form_contact %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= render 'management/tab-end' %>
