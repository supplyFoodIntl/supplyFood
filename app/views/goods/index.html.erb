<p id="notice"><%= notice %></p>

<h1>Goods</h1>

<table>
  <thead>
    <tr>
      <th>Expiration date</th>
      <th>Measure unit</th>
      <th>Good type</th>
      <th>Quantity</th>
      <th>Donation</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @goods.each do |good| %>
      <tr>
        <td><%= good.Expiration_date %></td>
        <td><%= good.measure_unit %></td>
        <td><%= good.good_type %></td>
        <td><%= good.quantity %></td>
        <td><%= good.donation %></td>
        <td><%= good.address %></td>
        <td><%= link_to 'Show', good %></td>
        <td><%= link_to 'Edit', edit_good_path(good) %></td>
        <td><%= link_to 'Destroy', good, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Good', new_good_path %>
