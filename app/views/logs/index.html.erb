<%= render 'management/tab'  %>
<p id="notice"><%= notice %></p>

<h2>Logs</h2>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Class name</th>
      <th scope="col">Method name</th>
      <th scope="col">Hash parms</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr scope="row">
        <td><%= log.class_name %></td>
        <td><%= log.method_name %></td>
        <td><%= log.hash_parms %></td>
        <td><%= link_to 'Show', log %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= render 'management/tab-end' %>