<%= render 'management/tab' %>
<p id="notice"><%= notice %></p>

<h2>Public Place Types</h2>

<table class="table">
  <thead>
    <tr>
      <th  scope="col">Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @public_place_types.each do |public_place_type| %>
      <tr scope="row">
        <td><%= public_place_type.name %></td>
        <td><%= link_to 'Show', public_place_type %></td>
        <td><%= link_to 'Edit', edit_public_place_type_path(public_place_type) %></td>
        <td><%= link_to 'Destroy', public_place_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Public Place Type', new_public_place_type_path %>
