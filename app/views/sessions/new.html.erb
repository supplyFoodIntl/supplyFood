<h1>Sign in</h1>
<% form_for(:session, :html => {class: "form-horizontal", role: "form"}, url: signin_path) do |form| %>  

  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :password_digest %>
    <%= form.text_field :password_digest %>
  </div>


  <div class="actions">
    <%= form.submit "Log in", class: 'btn btn-primary btn-lg'%>
  </div>

<% end %>