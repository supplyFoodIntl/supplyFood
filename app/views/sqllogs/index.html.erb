<%= render 'management/tab'  %>
<p id="notice"><%= notice %></p>

<h2>Sqllogs</h2>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Sql executed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sqllogs.each do |sqllog| %>
      <tr scope="row>
        <td><%= sqllog.sql_executed %></td>
        <td><%= link_to 'Show', sqllog %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= render 'management/tab-end' %>
