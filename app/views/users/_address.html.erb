<%= render 'tab' %>

<h1>New User - Address</h1>


<%= form_for @user do |form| %>

  <div class="field">
    <%= form.label :public_place_id %>
    <%= form.text_field :public_place_id %>
  </div>

  <div class="field">
    <%= form.label :private_place_number %>
    <%= form.number_field :private_place_number %>
  </div>

  <div class="field">
    <%= form.label :private_place_type_id %>
    <%= form.text_field :private_place_type_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>



